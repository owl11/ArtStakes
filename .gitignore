# Ignore node_modules directory
/node_modules/*

# Ignore everything in lib directory
/lib/*

# Include only lib/package.json
!/lib/package.json
# Ignore compiled output
/dist/
/out/
/build/

# Ignore environment-specific files
.env
.env.local
.env.*.local

# Ignore package-lock.json (use npm-shrinkwrap.json instead)
package-lock.json

# Ignore logs and temp files
*.log
*.tmp
